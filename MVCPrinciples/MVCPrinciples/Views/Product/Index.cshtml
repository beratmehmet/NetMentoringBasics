@model IEnumerable<Product>

<a asp-action ="Create" asp-controller="Product" class="btn btn-dark" role="button">Create</a>

<table>
    <tr>
        <th>Poduct ID</th>
        <th>Product Name</th>
        <th>Quantity Per Unit</th>
        <th>Unit Price</th>
        <th>Units In Stock</th>
        <th>Units On Order</th>
        <th>Reorder Level</th>
        <th>Discontinued</th>
        <th>Category</th>
        <th>Supplier</th>
    </tr>
    @foreach (var product in Model)
    {
        @await Html.PartialAsync("_ProductCard", product)
    }
</table>